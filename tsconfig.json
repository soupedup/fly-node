{
  "include": [
    "src/**/*"
  ],
  "compilerOptions": {                        
    "declaration": true,
    "target": "es6",
    "module": "commonjs",
    "outDir": "./dist",                      
    "rootDir": "./src",
    "strict": true,
    "lib": [
      "es2020",
      "esnext.asynciterable"
    ],
    "moduleResolution": "node"
  },
  "exclude":[
    "./node_modules"
  ],
  "esModuleInterop": true
}
